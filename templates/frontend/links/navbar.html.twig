							<div class="col-md-7 col-sm-7 col-xs-12">
								<div class="topbar-left d-flex">
									<div class="email">
										<i class="fa fa-envelope" aria-hidden="true"></i>Email: tivatheme@gmail.com
									</div>
									<div class="skype">
										<i class="fa fa-skype" aria-hidden="true"></i>Skype: tivatheme
									</div>
								</div>
							</div>
							
							<!-- Topbar Right -->
							<div class="col-md-5 col-sm-5 col-xs-12">
								<div class="topbar-right d-flex justify-content-end">
									<!-- My Account -->
									<div class="dropdown account">
										<div class="dropdown-toggle" data-toggle="dropdown">
											Mon compte
										</div>
										{% if app.user %}
											<div class="dropdown-menu">									
											<div class="item">
												<a href="#" title="Log in to your customer account"><i class="fa fa-cog"></i>Mon compte</a>
											</div>
											<div class="item">
												<a href="{{ path ('app_logout')}}" title="Log in to your customer account"><i class="fa fa-log-out"></i>Se deconnecter</a>
											</div>
											
											<div class="item">
												<a href="#" title="My Wishlists"><i class="fa fa-heart"></i>Mes Commmandes</a>
											</div>
											</div>
										{% else %}
										<div class="dropdown-menu">									
											
											<div class="item">
												<a href="{{ path ('app_login')}}" title="Log in to your customer account"><i class="fa fa-sign-in"></i>Se connecter</a>
											</div>
											<div class="item">
												<a href="{{ path ('app_register')}}" title="Register Account"><i class="fa fa-user"></i>S'inscrire</a>
											</div>
											
										</div>
										{% endif %}
										

									</div>
									
									<!-- Language -->
									<div class="dropdown language">
										<div class="dropdown-toggle" data-toggle="dropdown">
											<img src="{{ asset ('assets/img/language-en.jpg')}}" alt="Language English">
										</div>
										<div class="dropdown-menu">
											<div class="item">
												<a href="#" title="Language English"><img src="{{ asset ('assets/img/language-en.jpg')}}" alt="Language English"> English</a>
											</div>
											<div class="item">
												<a href="#" title="Language French"><img src="{{ asset ('assets/img/language-fr.jpg')}}" alt="Language French"> French</a>
											</div>
										</div>
									</div>
									
									<!-- Currency -->
									<div class="dropdown currency">
										<div class="dropdown-toggle" data-toggle="dropdown">
											USD
										</div>
										<div class="dropdown-menu">
											<div class="item">
												<a href="#" title="USD">USD</a>
											</div>
											<div class="item">
												<a href="#" title="EUR">EUR</a>
											</div>
