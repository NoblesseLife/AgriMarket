														{{ dump(categorie) }}
													</pre>
												{% endfor %}
													<li class="has-image">
														<img src="{{ asset ('assets/img/product/product-category-1.png')}}" alt="Product Category Image">
														<a href="product-grid-left-sidebar.html" title="Vegetables">Vegetables</a>
														</li>
													<li class="has-image">
														<img src="{{ asset ('assets/img/product/product-category-2.png')}}" alt="Product Category Image">
														<a href="product-grid-left-sidebar.html" title="Fruits">Fruits</a>
													</li>
													<li class="has-image">
														<img src="{{ asset ('assets/img/product/product-category-3.png')}}" alt="Product Category Image">
														<a href="product-grid-left-sidebar.html" title="Bread">Bread</a>
													</li>
													<li class="has-image">
														<img src="{{ asset ('assets/img/product/product-category-4.png')}}" alt="Product Category Image">
														<a href="product-grid-left-sidebar.html" title="Juices">Juices</a>
													</li>
													<li class="has-image">
														<img src="{{ asset ('assets/img/product/product-category-5.png')}}" alt="Product Category Image">
