										<li>
											<a href="page-contact.html">Contacts</a>
										</li>
									</ul>
								</div>
							</div>
							
							<div class="col-lg-3 col-md-3 col-sm-12 padding-0">
								<!-- Cart -->
								
								<div class="block-cart dropdown">
									<div class="cart-title">
										<i class="fa fa-shopping-basket"></i>
										<span class="cart-count"></span>
									</div>
									
